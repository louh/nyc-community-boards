    "leaflet-geocoder-mapzen": "1.7.1",
    "leaflet-hash": "0.2.1",
    "redux": "3.6.0",
    "redux-logger": "2.6.1",
    "turf-extent": "1.0.4",
    "turf-inside": "3.0.12",
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babelify": "7.3.0",
    "browser-sync": "2.16.1",
    "browserify": "13.1.0",
    "browserify-shim": "3.8.12",
    "cssnano": "3.7.5",
    "gulp": "3.9.1",
    "gulp-autoprefixer": "3.1.1",
    "gulp-cssimport": "3.1.0",
    "gulp-cssnano": "2.1.2",
    "gulp-rename": "1.2.2",
    "gulp-sass": "2.3.2",
    "gulp-sourcemaps": "1.6.0",
    "gulp-uglify": "2.0.0",
    "gulp-util": "3.0.7",
    "loose-envify": "1.2.0",
    "snazzy": "5.0.0",
    "standard": "8.3.0",
    "surge": "0.18.0",
    "topojson": "1.6.27",
    "vinyl-buffer": "1.0.0",
    "vinyl-source-stream": "1.1.0"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "loose-envify"
    ]
  },
  "browserify-shim": {
    "tangram": "global:Tangram"
  }
