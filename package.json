{
  "name": "nyc-community-boards",
  "version": "0.0.0",
  "description": "Find your neighborhood's community board",
  "author": "Lou Huang <lou@louhuang.com>",
  "license": "MIT",
  "homepage": "http://louhuang.com/nyc-community-boards",
  "repository": {
    "type": "git",
