{
  "name": "nyc-community-boards",
  "version": "0.0.0",
  "description": "Find your neighborhood's community board",
  "author": "Lou Huang <lou@louhuang.com>",
  "license": "MIT",
  "homepage": "http://louhuang.com/nyc-community-boards",
  "repository": {
    "type": "git",
    "url": "https://github.com/louh/nyc-community-boards.git"
  },
  "bugs": {
    "url": "https://github.com/louh/nyc-community-boards/issues"
  },
  "scripts": {
    "postinstall": "npm run build",
    "start": "gulp",
    "test": "standard ./src/js/**/*.js --verbose | snazzy",
    "build": "gulp build",
    "predeploy": "npm test && npm run build",
    "deploy": "gh-pages --dist public",
    "predeploy:surge": "npm run predeploy",
    "deploy:surge": "surge public --domain nyc-community-boards.surge.sh"
  },
  "main": "index.js",
  "dependencies": {
    "leaflet": "1.0.3",
    "leaflet-geocoder-mapzen": "1.8.0",
    "leaflet-hash": "0.2.1",
    "redux": "3.6.0",
    "redux-logger": "2.8.2",
    "topojson": "2.2.0",
    "@turf/bbox": "3.10.5",
    "@turf/inside": "3.10.5",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.23.0",
    "babelify": "7.3.0",
    "browser-sync": "2.18.8",
    "browserify": "14.1.0",
    "browserify-shim": "3.8.13",
    "cssnano": "3.10.0",
    "gh-pages": "0.12.0",
    "gulp": "3.9.1",
    "gulp-autoprefixer": "3.1.1",
    "gulp-cssimport": "5.0.0",
    "gulp-cssnano": "2.1.2",
    "gulp-rename": "1.2.2",
    "gulp-sass": "3.1.0",
    "gulp-sourcemaps": "2.4.1",
    "gulp-uglify": "2.1.0",
    "gulp-util": "3.0.8",
    "loose-envify": "1.3.1",
    "snazzy": "6.0.0",
    "standard": "9.0.1",
    "surge": "0.18.0",
    "vinyl-buffer": "1.0.0",
    "vinyl-source-stream": "1.1.0"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "loose-envify"
    ]
  },
  "browserify-shim": {
    "tangram": "global:Tangram"
  }
}
